{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus data source",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": "9.0.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": "9.0.0"
    },
    {
      "type": "panel",
      "id": "gauge",
      "name": "Gauge",
      "version": "9.0.0"
    },
    {
      "type": "panel",
      "id": "barchart",
      "name": "Bar chart",
      "version": "9.0.0"
    },
    {
      "type": "panel",
      "id": "table",
      "name": "Table",
      "version": "9.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Machine Learning-based leak detection for NightOwl water monitoring",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["nightowl"],
      "targetBlank": false,
      "title": "NightOwl Main Dashboard",
      "tooltip": "",
      "type": "dashboards",
      "url": ""
    }
  ],
  "panels": [
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "panels": [],
      "title": "ü§ñ ML Leak Detection",
      "type": "row"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Probability of a water leak based on ML Isolation Forest model",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "mappings": [],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 25 },
              { "color": "orange", "value": 50 },
              { "color": "red", "value": 70 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 5, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "nightowl_ml_leak_probability{device_name=~\"$device\"}",
          "legendFormat": "ML Leak Probability",
          "refId": "A"
        }
      ],
      "title": "ML Leak Probability",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Isolation Forest anomaly score. Negative = anomaly, Positive = normal",
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 3,
          "mappings": [],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": -0.1 },
              { "color": "green", "value": 0 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 5, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "text": {},
        "wideLayout": true
      },
      "targets": [
        {
          "expr": "nightowl_ml_anomaly_score{device_name=~\"$device\"}",
          "legendFormat": "Score",
          "refId": "A"
        }
      ],
      "title": "Anomaly Score",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Is the ML model detecting an anomaly right now?",
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [
            {
              "options": {
                "0": { "color": "green", "index": 0, "text": "NORMAL" },
                "1": { "color": "red", "index": 1, "text": "ANOMALY!" }
              },
              "type": "value"
            }
          ],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.5 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 9, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "text": { "titleSize": 16, "valueSize": 32 }
      },
      "targets": [
        {
          "expr": "nightowl_ml_is_anomaly{device_name=~\"$device\"}",
          "legendFormat": "Status",
          "refId": "A"
        }
      ],
      "title": "ML Status",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Model confidence based on training data quality (0=low, 1=medium, 2=high)",
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [
            {
              "options": {
                "0": { "color": "orange", "index": 0, "text": "LOW" },
                "1": { "color": "yellow", "index": 1, "text": "MEDIUM" },
                "2": { "color": "green", "index": 2, "text": "HIGH" }
              },
              "type": "value"
            }
          ],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "orange", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "green", "value": 2 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 3, "x": 13, "y": 1 },
      "id": 5,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "text": {}
      },
      "targets": [
        {
          "expr": "nightowl_ml_model_confidence{device_name=~\"$device\"}",
          "legendFormat": "Confidence",
          "refId": "A"
        }
      ],
      "title": "Model Confidence",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "When the ML model was last trained",
      "fieldConfig": {
        "defaults": {
          "unit": "dateTimeAsIso",
          "decimals": 0,
          "mappings": [],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 5, "y": 9 },
      "id": 40,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "text": {}
      },
      "targets": [
        {
          "expr": "nightowl_ml_model_trained_at * 1000",
          "legendFormat": "Trained",
          "refId": "A"
        }
      ],
      "title": "Model Trained",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Number of samples used to train the ML model",
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "mappings": [],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "orange", "value": null },
              { "color": "yellow", "value": 10000 },
              { "color": "green", "value": 20000 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 9, "y": 9 },
      "id": 41,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "text": {}
      },
      "targets": [
        {
          "expr": "nightowl_ml_model_training_samples",
          "legendFormat": "Samples",
          "refId": "A"
        }
      ],
      "title": "Training Samples",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Current ML model version",
      "fieldConfig": {
        "defaults": {
          "unit": "string",
          "mappings": [],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 3, "x": 13, "y": 9 },
      "id": 42,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "text": {}
      },
      "targets": [
        {
          "expr": "nightowl_ml_model_version_info",
          "legendFormat": "{{version}}",
          "refId": "A"
        }
      ],
      "title": "Model Version",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Comparison of ML vs statistical leak detection",
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [
            {
              "options": {
                "0": { "color": "green", "index": 0, "text": "No Leak" },
                "1": { "color": "red", "index": 1, "text": "LEAK!" }
              },
              "type": "value"
            }
          ],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.5 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "id": 6,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "text": { "titleSize": 14, "valueSize": 24 }
      },
      "targets": [
        {
          "expr": "nightowl_ml_is_anomaly{device_name=~\"$device\"}",
          "legendFormat": "ML Detection",
          "refId": "A"
        },
        {
          "expr": "nightowl:leak_detected{device_name=~\"$device\"}",
          "legendFormat": "Stats Detection",
          "refId": "B"
        }
      ],
      "title": "ML vs Stats Detection",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Time since last ML inference ran",
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "mappings": [],
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 300 },
              { "color": "red", "value": 600 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 5, "y": 5 },
      "id": 7,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "text": {}
      },
      "targets": [
        {
          "expr": "time() - nightowl_ml_inference_timestamp{device_name=~\"$device\"}",
          "legendFormat": "Age",
          "refId": "A"
        }
      ],
      "title": "Last Inference",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Total ML inferences by status",
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "mappings": [],
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "success" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "failure" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 4, "w": 4, "x": 9, "y": 5 },
      "id": 8,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "text": {}
      },
      "targets": [
        {
          "expr": "sum(increase(nightowl_ml_inferences_total{device_id=~\".*\"}[1h])) by (status)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "title": "Inferences (1h)",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 10,
      "panels": [],
      "title": "üìà ML Trends",
      "type": "row"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "ML leak probability over time",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Probability %",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": true,
            "thresholdsStyle": { "mode": "line" }
          },
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 25 },
              { "color": "orange", "value": 50 },
              { "color": "red", "value": 70 }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "id": 11,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "expr": "nightowl_ml_leak_probability{device_name=~\"$device\"}",
          "legendFormat": "ML Probability",
          "refId": "A"
        },
        {
          "expr": "nightowl:leak_probability_score{device_name=~\"$device\"}",
          "legendFormat": "Stats Probability",
          "refId": "B"
        }
      ],
      "title": "Leak Probability: ML vs Statistical",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Isolation Forest anomaly score over time",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Score",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": true,
            "thresholdsStyle": { "mode": "line+area" }
          },
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "transparent", "value": null },
              { "color": "red", "value": null }
            ]
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Anomaly Threshold" },
            "properties": [
              { "id": "custom.lineStyle", "value": { "dash": [10, 5], "fill": "dash" } },
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } },
              { "id": "custom.fillOpacity", "value": 0 }
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "id": 12,
      "options": {
        "legend": { "calcs": ["mean", "min"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "nightowl_ml_anomaly_score{device_name=~\"$device\"}",
          "legendFormat": "Anomaly Score",
          "refId": "A"
        },
        {
          "expr": "0",
          "legendFormat": "Anomaly Threshold",
          "refId": "B"
        }
      ],
      "title": "ML Anomaly Score Trend",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 20,
      "panels": [],
      "title": "üîç Feature Analysis",
      "type": "row"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Top features contributing to the current anomaly score",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisPlacement": "auto",
            "fillOpacity": 80,
            "gradientMode": "hue",
            "lineWidth": 1,
            "scaleDistribution": { "type": "linear" }
          },
          "color": { "mode": "palette-classic" },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "id": 21,
      "options": {
        "barRadius": 0.1,
        "barWidth": 0.8,
        "groupWidth": 0.7,
        "legend": { "displayMode": "list", "placement": "right" },
        "orientation": "horizontal",
        "showValue": "always",
        "stacking": "none",
        "tooltip": { "mode": "single", "sort": "none" },
        "xTickLabelRotation": 0,
        "xTickLabelSpacing": 100
      },
      "targets": [
        {
          "expr": "topk(10, abs(nightowl_ml_feature_contribution{device_name=~\"$device\"}))",
          "format": "table",
          "instant": true,
          "legendFormat": "{{feature}}",
          "refId": "A"
        }
      ],
      "title": "Top Feature Contributions",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "device_id": true,
              "device_name": true,
              "instance": true,
              "job": true
            },
            "renameByName": {
              "feature": "Feature",
              "Value": "Contribution"
            }
          }
        }
      ],
      "type": "barchart"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Current values of key ML input features",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          },
          "mappings": [],
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [
              { "id": "custom.width", "value": 100 },
              { "id": "decimals", "value": 3 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Feature" },
            "properties": [{ "id": "custom.width", "value": 250 }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "id": 22,
      "options": {
        "cellHeight": "sm",
        "footer": { "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Value" }]
      },
      "targets": [
        {
          "expr": "nightowl_ml_feature_value{device_name=~\"$device\"}",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "ML Feature Values",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "device_id": true,
              "device_name": true,
              "instance": true,
