      {
        "id": 9,
        "title": "ðŸš¨ Leak Detection",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 23},
        "collapsed": false
      },
      {
        "id": 10,
        "title": "Leak Probability Score",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 24},
        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "nightowl:leak_probability_score or clamp_max(((avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[1h]) / (avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d]) + 0.01) - 1) > 0.5) * 30 + ((avg_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) - nightowl_telemetry_value{key=\"S1\"}) / (stddev_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) + 0.1) > 2.5) * 40 + (hour() >= 23 or hour() < 5) * (nightowl_telemetry_value{key=\"P1C1\"} > 0.5) * 30, 100)",
            "legendFormat": "Leak Score",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "graphMode": "area",
          "colorMode": "background",
          "justifyMode": "center",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 0,
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 30, "color": "yellow"},
                {"value": 50, "color": "orange"},
                {"value": 70, "color": "red"}
              ]
            }
          }
        },
        "description": "Composite leak probability based on pump duty cycle anomaly (30%), pressure drop (40%), and night-time pump activity (30%). Score â‰¥70% indicates likely leak."
      },
      {
        "id": 11,
        "title": "Leak Indicators Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 24},
        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "label_replace(((avg_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) - nightowl_telemetry_value{key=\"S1\"}) / (stddev_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) + 0.1) > 2.5) * 100, \"indicator\", \"Pressure Drop\", \"\", \"\")",
            "legendFormat": "",
            "refId": "A",
            "instant": true
          },
          {
            "expr": "label_replace(((avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[1h]) - avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d])) / (stddev_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d]) + 0.01) > 2.5) * 100, \"indicator\", \"Excessive Pump Runtime\", \"\", \"\")",
            "legendFormat": "",
            "refId": "B",
            "instant": true
          },
          {
            "expr": "label_replace((hour() >= 23 or hour() < 5) * (nightowl_telemetry_value{key=\"P1C1\"} > 0.5) * 100, \"indicator\", \"Night Pump Activity (11pm-5am)\", \"\", \"\")",
            "legendFormat": "",
            "refId": "C",
            "instant": true
          },
          {
            "expr": "label_replace(((avg_over_time(nightowl_telemetry_value{key=\"Level1_precent\"}[8d]) - nightowl_telemetry_value{key=\"Level1_precent\"}) / (stddev_over_time(nightowl_telemetry_value{key=\"Level1_precent\"}[8d]) + 0.1) > 2.5) * 100 or label_replace(vector(0), \"indicator\", \"Rapid Level Drop (awaiting data)\", \"\", \"\"), \"indicator\", \"Rapid Level Drop (awaiting data)\", \"\", \"\")",
            "legendFormat": "",
            "refId": "D",
            "instant": true
          },
          {
            "expr": "label_replace(((increase(nightowl_telemetry_value{key=\"Pulse_TotalGallons\"}[24h]) - avg_over_time(increase(nightowl_telemetry_value{key=\"Pulse_TotalGallons\"}[24h])[8d:1d])) / (stddev_over_time(increase(nightowl_telemetry_value{key=\"Pulse_TotalGallons\"}[24h])[8d:1d]) + 1) > 2.5) * 100 or label_replace(vector(0), \"indicator\", \"Excessive Water Usage (awaiting data)\", \"\", \"\"), \"indicator\", \"Excessive Water Usage (awaiting data)\", \"\", \"\")",
            "legendFormat": "",
            "refId": "E",
            "instant": true
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": []
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left",
              "filterable": false,
              "width": null
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "red"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "âœ“ Normal"},
              {"type": "value", "value": "100", "text": "âš ï¸ ALERT"}
            ],
            "color": {
              "mode": "thresholds"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "indicator"},
              "properties": [
                {"id": "custom.width", "value": 300},
                {"id": "displayName", "value": "Indicator"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Value"},
              "properties": [
                {"id": "custom.width", "value": 150},
                {"id": "displayName", "value": "Status"},
                {"id": "custom.cellOptions", "value": {"type": "color-background"}}
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "job": true,
                "instance": true,
                "device_id": true,
                "device_name": true,
                "key": true
              },
              "indexByName": {
                "indicator": 0,
                "Value": 1
              },
              "renameByName": {
                "indicator": "Leak Indicator",
                "Value": "Status"
              }
            }
          },
          {
            "id": "sortBy",
            "options": {
              "fields": {},
              "sort": [
                {
                  "field": "Status",
                  "desc": true
                }
              ]
            }
          }
        ],
        "description": "Leak detection indicators: Green = Normal, Red = Alert condition detected"
      },
      {
        "id": 15,
        "title": "Leak Detection Trend (8 Days)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "nightowl:leak_probability_score or clamp_max(((avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[1h]) / (avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d]) + 0.01) - 1) > 0.5) * 30 + ((avg_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) - nightowl_telemetry_value{key=\"S1\"}) / (stddev_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) + 0.1) > 2.5) * 40 + (hour() >= 23 or hour() < 5) * (nightowl_telemetry_value{key=\"P1C1\"} > 0.5) * 30, 100)",
