        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "max_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d])",
            "legendFormat": "Max Current",
            "refId": "A"
          },
          {
            "expr": "avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d])",
            "legendFormat": "Avg Current",
            "refId": "B"
          },
          {
            "expr": "min_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d])",
            "legendFormat": "Min Current",
            "refId": "C"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
