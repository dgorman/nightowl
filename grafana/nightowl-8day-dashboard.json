        "collapsed": false
      },
      {
        "id": 2,
        "title": "Current Pressure vs 8-Day Average",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1},
        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "nightowl_telemetry_value{key=\"S1\"}",
            "legendFormat": "Current",
            "refId": "A"
          },
          {
            "expr": "avg_over_time(nightowl_telemetry_value{key=\"S1\"}[8d])",
            "legendFormat": "8-Day Avg",
            "refId": "B"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name",
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "pressurepsi",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 35, "color": "yellow"},
                {"value": 30, "color": "red"}
              ]
            }
          }
        }
      },
