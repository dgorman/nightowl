          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "pressurepsi",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 35, "color": "yellow"},
                {"value": 30, "color": "red"}
