        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 17},
        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "nightowl_telemetry_value{key=\"S1\"}",
            "legendFormat": "Pressure",
            "refId": "A"
          },
          {
            "expr": "nightowl_telemetry_value{key=\"P1C1\"}",
