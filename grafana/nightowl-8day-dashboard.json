        "title": "Pump Duty Cycle Anomaly",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 10, "y": 24},
        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "(avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[1h]) - avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d])) / (stddev_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d]) + 0.01)",
