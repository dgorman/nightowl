      {
        "id": 9,
        "title": "ðŸš¨ Leak Detection",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 23},
        "collapsed": false
      },
      {
        "id": 10,
        "title": "Leak Probability Score",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 24},
        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "nightowl:leak_probability_score or clamp_max(((avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[1h]) / (avg_over_time(nightowl_telemetry_value{key=\"P1C1\"}[8d]) + 0.01) - 1) > 0.5) * 30 + ((avg_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) - nightowl_telemetry_value{key=\"S1\"}) / (stddev_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) + 0.1) > 2.5) * 40 + (hour() >= 23 or hour() < 5) * (nightowl_telemetry_value{key=\"P1C1\"} > 0.5) * 30, 100)",
            "legendFormat": "Leak Score",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "graphMode": "area",
          "colorMode": "background",
          "justifyMode": "center",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 0,
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 30, "color": "yellow"},
                {"value": 50, "color": "orange"},
                {"value": 70, "color": "red"}
              ]
            }
          }
        },
        "description": "Composite leak probability based on pump duty cycle anomaly (30%), pressure drop (40%), and night-time pump activity (30%). Score â‰¥70% indicates likely leak."
      },
      {
        "id": 11,
        "title": "Pressure Drop Anomaly",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 6, "y": 24},
        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "(avg_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) - nightowl_telemetry_value{key=\"S1\"}) / (stddev_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) + 0.1)",
            "legendFormat": "Pressure Z-Score",
            "refId": "A"
