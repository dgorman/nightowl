        "title": "Pressure Drop Detection (Z-Score)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 17},
        "datasource": {"uid": "af1z6oz34c7b4f", "type": "prometheus"},
        "targets": [
          {
            "expr": "(avg_over_time(nightowl_telemetry_value{key=\"S1\"}[8d]) - nightowl_telemetry_value{key=\"S1\"}) / stddev_over_time(nightowl_telemetry_value{key=\"S1\"}[8d])",
            "legendFormat": "Pressure Z-Score",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
